<!-- views/home.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Home</title>
</head>
<body>
  <h1>Welcome <%= user.displayName || user.username %>!</h1>
  <p>Your GitHub ID: <%= user.id %></p>
  <p>Your GitHub URL: <a href="<%= user.profileUrl %>"><%= user.profileUrl %></a></p>
  <img src="<%= user.photos && user.photos[0] && user.photos[0].value %>" alt="Profile Picture">
  <a href="/logout">Logout</a>
</body>
</html>
